{
  "name": "dreamtester-2.0",
  "productName": "Dreamtester 2.0",
  "version": "1.0.0",
  "description": "A quantitative backtesting and trading application.",
  "author": "Prashanth Reddy GP",
  "main": "electron/main.js",
  "scripts": {
    "start": "concurrently -k \"npm:dev:frontend\" \"npm:dev:electron\"",
    "dev:frontend": "vite frontend --config frontend/vite.config.js",
    "dev:electron": "wait-on tcp:5173 && cross-env ELECTRON_START_URL=http://localhost:5173 electron-forge start",
    "package": "electron-forge package",
    "make": "npm run build:frontend && npm run build:backend && electron-forge make",
    "build:frontend": "vite build frontend --config frontend/vite.config.js",
    "build:backend": "backend\\.venv\\Scripts\\activate && pyinstaller backend/main.py --paths backend --add-data \"backend/core;core\" --hidden-import=appdirs --noconfirm --onefile --name=backend_app --distpath=backend/dist"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.7.0",
    "@mui/x-date-pickers": "^8.6.0",
    "@tauri-apps/api": "^2.6.0",
    "@types/jszip": "^3.4.0",
    "@types/react-window": "^1.8.8",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "electron-squirrel-startup": "^1.0.1",
    "framer-motion": "^12.23.0",
    "jszip": "^3.10.1",
    "lightweight-charts": "^5.0.9",
    "lucide": "^0.525.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-resizable-panels": "^3.0.3",
    "react-router-dom": "^7.6.3",
    "react-window": "^1.8.11",
    "reactflow": "^11.11.4",
    "recharts": "^3.2.0",
    "simple-statistics": "^7.8.8",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "37.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "wait-on": "^8.0.3"
  }
}
